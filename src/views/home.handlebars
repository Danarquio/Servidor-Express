<h1>Productos</h1>
<div class="shop">

    {{#each products}}
    <div class="productos">

        <img class="vuelta" src={{this.carru1}} alt={{this.title}} />
        <div class="infoCard">

            <div class="textosprod">

                <h2 class="productos__nombre">{{this.title}}</h2>
                <div>
                    <h2 class="productos__precio">${{this.price}}</h2>
                    <h4 class="productos__gramo">x {{this.minimo}}gramos</h4>
                </div>
            </div>

        </div>


    </div>
    {{/each}}

</div>